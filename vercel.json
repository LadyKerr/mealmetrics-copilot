{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server.js"
      },
      {
        "src": "/(.*)",
        "dest": "build/$1"
      }
    ],
    "env": {
      "OPENAI_API_KEY": "process.env.OPENAI_API_KEY"
    }
  }